{
  "$schema": "./schema.json",
  "version": 2,
  "axioms": {
    "packagers": "packager"
  },
  "rules": {
    "binary-exec-lib": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/*.jar",
            "**/*.exe",
            "**/*.dll",
            "**/*.pyc",
            "**/*.class",
            "**/*.so",
            "**/*.o",
            "**/*.a"
          ],
          "nocase": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Binary-Exe-Lib"
    },
    "binary-archive": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/*.zip",
            "**/*.tar",
            "**/*.tar.gz",
            "**/*.7z",
            "**/*.iso",
            "**/*.rpm",
            "**/*.gz",
            "**/*.rar",
            "**/*.whl",
            "**/*.tgz"
          ],
          "nocase": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Binary-Archive"
    },
    "binary-document": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/*.pdf",
            "**/*.doc",
            "**/*.docx",
            "**/*.xls",
            "**/*.xlsx",
            "**/*.ppt",
            "**/*.pptx",
            "**/*.odt"
          ],
          "nocase": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Binary-Document"
    },
    "amazon-logo": {
      "level": "warning",
      "rule": {
        "type": "file-hashes-not-exist",
