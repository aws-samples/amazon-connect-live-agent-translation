      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Amazon-Logo"
    },
    "third-party-image": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/*.jpg",
            "**/*.jpeg",
            "**/*.png",
            "**/*.gif",
            "**/*.tiff",
            "**/*.ico",
            "**/*.svg"
          ],
          "nocase": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Third-Party-Image"
    },
    "dataset": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/*.csv",
            "**/*.data",
            "**/*.tsv"
          ],
          "nocase": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Dataset"
    },
    "dockerfile": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/*docker*"
          ],
          "nocase": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Dockerfile"
    },
    "dockerfile-download-statement": {
      "level": "warning",
      "rule": {
        "type": "file-not-contents",
        "options": {
          "globsAll": [
            "**/*docker*"
          ],
          "contents": [
            "from",
            "wget",
            "curl",
            "git clone"
          ],
          "nocase": true,
          "flags": "gi",
          "fail-on-non-existent": false,
          "display-result-context": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Dockerfile-Download-Statement/"
    },
    "internal-url": {
      "level": "warning",
      "rule": {
        "type": "file-not-contents",
        "options": {
          "globsAll": [
            "**/*"
          ],
          "contents": [
            "http[s]?:\/\/s3.amazonaws.com",
            "http[s]?:\/\/w.amazon",
            "http[s]?:\/\/t.amazon",
            "http[s]?:\/\/quip-amazon.com",
            "corp.amazon"
          ],
          "nocase": true,
          "flags": "gi",
          "fail-on-non-existent": false,
          "display-result-context": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Internal-URL"
    },
    "prohibited-license": {
      "level": "warning",
      "rule": {
        "type": "file-not-contents",
        "options": {
          "globsAll": [
            "**/*"
          ],
          "contents": [
            "Affero",
            "Version 1, March 2002",
            "AGPL",
            "GNU Affero General Public License",
            "Common Public Attribution License",
            "Honest Public License",
            "Open Software License",
            "RealNetworks Public Source License",
            "version 3 of the License",
            "either version 3",
            "Version 3, 29 June 2007",
            "version 3 of the GNU General Public License",
            "version 3 of the GNU Lesser General Public License",
            "GPL 3.0",
            "GPL3",
            "GPL-3",
            "GPL version 3",
            "General Public License v3",
            "NonCommercial",
            "Additional Grant of Patent Rights Version",
            "subject to the Facebook Developer Principles",
            "and not-for-profit purposes",
            "for working with ICE",
            "Elastic License",
            "Commercial License",
            "Proprietary License",
            "due to their collaboration with US Immigration"
          ],
          "nocase": true,
          "flags": "gi",
          "fail-on-non-existent": false,
          "display-result-context": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Prohibited-License/"
    },
    "third-party-license-file": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/LICENSE*",
            "**/COPYING*",
            "**/COPYRIGHT*",
            "**/GPL*",
            "**/THANK*",
            "**/PATENT*"
          ],
          "nocase": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Third-Party-License-File/"
    },
    "hidden-or-generated-file": {
      "level": "warning",
      "rule": {
        "type": "file-not-exists",
        "options": {
          "globsAll": [
            "**/.*",
            "**/*generated*",
            "**/*pycache*",
            "**/*target*",
            "**/out*",
            "**/dist*"
          ],
          "nocase": true,
          "dirs": true
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Hidden-Generated-File"
    },
    "large-file": {
      "level": "warning",
      "rule": {
        "type": "large-file",
        "options": {
          "globsAll": [
            "**/*"
          ],
          "size": 500000
        }
      },
      "policyUrl": "https://w.amazon.com/bin/view/Open_Source/Tools/Repolinter/Ruleset/Large-File"
    }
  }
}
